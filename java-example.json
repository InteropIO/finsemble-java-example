{
	"components": {
		"Java Example (local)": {
			"window": {
				"id": "JavaExample",
				"windowType": "FinsembleNativeWindow",
				"path": "/path/to/javaw",
				"arguments": "-jar $javaExampleJarRoot/JavaExample.jar",
				"defaultHeight": 600,
				"autoShow": true,
				"alwaysOnTop": false,
				"resizable": true,
				"showTaskbarIcon": false,
				"contextMenu": true,
				"addToWorkspace": true
			},
			"component": {
				"spawnOnStartup": false
			},
			"foreign": {
				"services": {
					"workspaceService": {
						"allowAutoArrange": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": true
					},
					"Window Manager": {
						"persistWindowState": false,
						"FSBLHeader": true
					},
					"Toolbar": {
						"iconURL": "$applicationRoot/assets/img/notepad.png"
					}
				}
			}
		},
